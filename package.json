    "express": "^4.17.1",
    "mongodb": "^3.6.10",
    "mongoose": "^5.13.2",
    "postman": "^0.2.0",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
